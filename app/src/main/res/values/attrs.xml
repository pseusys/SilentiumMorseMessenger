<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SilentInputView">
        <attr name="rotate" format="boolean" />
        <attr name="prefs" format="enum">
            <enum name="settings" value="0"/>
            <enum name="keyboard" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DescriptiveRecyclerView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="RepeatButton">
        <attr name="initialInterval" format="integer" />
        <attr name="normalInterval" format="integer" />
    </declare-styleable>
</resources>
