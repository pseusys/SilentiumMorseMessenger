<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:key="@string/pref_additional_key"
        app:title="Additional"
        app:iconSpaceReserved="false" >

        <Preference
            app:key="@string/pref_additional_feedback_key"
            app:title="Send feedback"
            app:persistent="false"
            app:summary="Report technical issues or suggest new features"
            app:icon="@drawable/icon_logo" >

            <intent android:action="android.intent.action.VIEW" android:data="https://github.com/pseusys/SilentiumMorseMessenger" />

        </Preference>

    </PreferenceCategory>

</PreferenceScreen>
